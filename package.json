{
  "name": "Heat",
  "version": "1.0.0",
  "description": "Testing Practice",
  "main": "server.js",
  "homepage": "http://greggarrett.github.io/Heat",
  "scripts": {
    "test": "mocha server/tests",
    "install-all": "concurrently \"npm install\" \"cd client && npm install\"",
    "client": "cd client && npm start",
    "server": "nodemon server/server.js",
    "dev": "concurrently \"nodemon server/server.js\" \"cd client && npm run-script start\"",
    "build": "concurrently \"npm install\" \"cd client && npm install\" && concurrently \"nodemon server/server.js\" \"cd client && npm run-script start\"",
    "start": "node server/server.js",
    "predeploy": "npm run-script build",
    "deploy": "gh-pages -d build"
  },
  "author": "Medhir Bhargava & Alex Webber",
  "license": "MIT",
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "should": "^13.2.3"
  },
  "dependencies": {
    "all": "0.0.0",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "express": "^4.17.1",
    "googleapis": "^71.0.0",
    "mocha": "^6.2.1",
    "moment": "^2.29.1",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.10",
    "react": "^17.0.2",
    "react-big-calendar": "^0.33.2",
    "react-bootstrap-validation": "^0.1.11",
    "react-dom": "^17.0.2",
    "react-moment": "^1.1.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "request": "^2.67.0",
    "should": "^13.2.3",
    "gh-pages": "^3.1.0",
    "superagent": "^6.1.0"
  }
}
