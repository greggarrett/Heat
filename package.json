  "license": "MIT",
  "devDependencies": {
    "should": "^7.1.1"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "express": "^4.17.1",
    "mocha": "^6.2.1",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.10",
    "react-scripts": "^4.0.3",
    "request": "^2.67.0",
    "should": "^13.2.3",
    "supertest": "^4.0.2"
  }
}
